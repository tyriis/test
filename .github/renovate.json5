{
  enabled: true,
  extends: [
    "github>techtales-io/renovate-config:base.json5",
    "github>techtales-io/renovate-config//dependencies/minecraft.json5",
    "github>tyriis/renovate-config//generic-regex-manager.json5", //  TODO: reconfigure
  ],
  ignorePaths: [
  ],
  includePaths: [
    // "terraform/images.yaml",
    "kubernetes/apps/gaming/minecraft-java/**",
  ],
  rebaseWhen: "conflicted",
  ansible: {
    enabled: false,
  },
  vulnerabilityAlerts: {
    enabled: false,
  },
  packageRules: [
    {
      matchDatasources: ["custom.luckpermsBukkit", "custom.luckpermsVelocity"],
      semanticCommitScope: "minecraft",
      commitMessageTopic: "update plugin {{depName}}",
    },
  ],
  customManagers: [
    {
      customType: "regex",
      managerFilePatterns: ["/^kubernetes/.+/plugins\\.txt$/"],
      //managerFilePatterns: [
      //  "/^kubernetes/apps/gaming/minecraft-java/.*/config/plugins\\.txt$/",
      //],
      matchStrings: [
        "(?<currentDigest>https://download\\.luckperms\\.net/\\d+/bukkit/loader/LuckPerms-Bukkit-(?<currentValue>[\\d.]+)\\.jar)",
      ],
      datasourceTemplate: "custom.luckpermsBukkit",
      depNameTemplate: "luckperms-bukkit",
      autoReplaceStringTemplate: "{{{newDigest}}}",
    },
    {
      customType: "regex",
      managerFilePatterns: ["^(kubernetes)/.+/plugins\\.txt$"],
      matchStrings: [
        "(?<currentDigest>https://download\\.luckperms\\.net/\\d+/velocity/LuckPerms-Velocity-(?<currentValue>[\\d.]+)\\.jar)",
      ],
      datasourceTemplate: "custom.luckpermsVelocity",
      depNameTemplate: "luckperms-velocity",
      autoReplaceStringTemplate: "{{{newDigest}}}",
    },
  ],
  customDatasources: {
    luckpermsBukkit: {
      defaultRegistryUrlTemplate: "https://metadata.luckperms.net/data/all",
      format: "json",
      transformTemplates: [
        '{ "releases": [ { "version": version, "digest": downloads.bukkit } ] }',
      ],
    },
    luckpermsVelocity: {
      defaultRegistryUrlTemplate: "https://metadata.luckperms.net/data/all",
      format: "json",
      transformTemplates: [
        '{ "releases": [ { "version": version, "digest": downloads.velocity, "url": "https://luckperms.net/" } ] }',
      ],
    },
  },
  jenkins: {
    enabled: false,
  },
}
